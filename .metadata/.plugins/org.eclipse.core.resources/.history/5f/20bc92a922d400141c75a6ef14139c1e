package co.com.eafit.conferre.conferencias.data.base;

import java.util.Collection;

public interface GenericDAO {
	
	public GenericDTO crear(GenericDTO parametro);
	public Collection<GenericDTO> recuperar(GenericDTO parametros);
	public GenericDTO update(GenericDTO nuevoObjeto);
	public int borrar(GenericDTO objetoaBorrar);
	
}
